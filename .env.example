# PayServeMe Environment Variables Template
# Copy this file to .env and fill in your values

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api

# Third-party Services
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key_here
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id_here

# Analytics
REACT_APP_GA_TRACKING_ID=your_google_analytics_id_here

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false